{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AzureAppConfigurationLabel": "Patents.ArtRepoCloud.Service",
  "AppSettings": {
    "MaxConcurrentCalls": 2,
    "EnableFamilyImport": true,
    "EnableExtendedFamilyImport": true
  },
  "UsptoApiSettings": {
    "Assignments": "retrieval/public/v1/assignments?applicationNumber=",
    "Applications": "retrieval/public/v2/applications/{0}/data",
    "ApplicationDataUrl": "retrieval/public/v2/application/data",
    "DocumentsV2Url": "retrieval/public/v2/documents",
    "TransactionHistoryUrl": "retrieval/public/v1/applications/{0}/transaction-history",
    "ContinuityUrl": "retrieval/public/v1/applications/{0}/continuity",
    "AddressesUrl": "retrieval/public/v2/applications/{0}/addresses",
    "DocumentsUrl": "retrieval/public/v1/documents",
    "DocumentsMetadataUrl": "retrieval/public/v1/applications/sdwp/external/metadata/"
  },
  "PairSettings": {
    "PdfZipMaxPageCount": 1000,
    "PdfInSingleFileMaxPageCount": 1000
  },
  "AwsInstanceSettings": {
    "EnableEc2Manager": true,
    "StartTime": "08:00:00",
    "StopTime": "20:00:00",
    "StopOnWeekend": true,
    "Timeout": 5,
    "MaximumDocumentsCount": 350,
    "MaximumProcessCount": 450,
    "MaxUnitsOfWork": 50
  },
  "EpoSettings": {
    "EpoKey": "",
    "EpoSecret": "",
    "EpoServiceUrl": "https://ops.epo.org/3.2/",
    "PublishedDataQueryString": "rest-services/published-data/publication/epodoc/{referenceNumber}/{command}",
    "PageLinkQueryString": "rest-services/{command}"
  },
  "IpdIfiApiSettings": {
    "DocumentUrl": "api/document",
    "ImageAttachmentsUrl": "api/document/attachments",
    "AttachmentFetchAllWeightRU": 5.5
  },
  "IficlaimsSettings": {
    "IfiUserName": "",
    "IfiPassword": "",
    "IfiServiceUrl": "https://cdws21.ificlaims.com",
    "IfiAttachmentFetchAllWeightRU": 5.5,
    "IfiDelayInSecondsForHttp429": 5,
    "IfiRetryCountForHttp429": -1,
    "IfiRetryOperationTimeoutInSeconds": 1800,
    "Ucid": "search/query?q=pn:{referenceNumber} AND pnctry:{countryCode}&sort=pd asc&fl=ucid",
    "BibDocument": "text/fetch?ucid={ucid}",
    "AttachmentList": "attachment/list/?ucid={ucid}",
    "AttachmentFetch": "attachment/fetch/?path={ifiPath}",
    "AttachmentFetchAll": "attachment/fetchall?ucid={ucid}",
    "FamilySimple": "search/query?q=fam:{familyId}&fl=ucid,pd,pn,pri,prid,an,ad,ttl_en&rows=100000000&sort=pd asc",
    "FamilyExtended": "/family/extended?ucid={ucid}"
  },
  "QuestelSettings": {
    "QuestelLogon": "",
    "QuestelPassword": "",
    "QuestelUrl": "http://pdf.orbit.com/GetPDF"
  },
  "FetcherSettings": {
    "EnableIpdIfiDocumentProvider": true,
    "EnableIpdIfiImageFetcher": true,
    "EnableIfiDocumentProvider": false,
    "EnableIfiPdfProvider": true,
    "EnableIfiImageProvider": false,
    "EnableEpoDocumentProvider": true,
    "EnableEpoPdfProvider": true,
    "EnableEpoImageProvider": true,
    "EnableQuestelPdfProvider": false,
    "EnableUsptoDocumentProvider": false
  },
  "AwsSettings": {
    "AwsAccessKey": "",
    "AwsSecretKey": ""
  }
}